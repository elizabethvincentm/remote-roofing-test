    "@react-mock/state": "^0.1.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.2.1",
    "autoprefixer": "^9.8.0",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
