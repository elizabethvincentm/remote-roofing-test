    "react-scripts": "3.4.1",
    "tailwindcss": "^1.4.6",
    "tailwindcss-aspect-ratio": "^3.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
